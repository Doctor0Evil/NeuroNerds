    { term: "qpu_math_enforce", description: "All calculations, review, and output logic must use QPU.Math for mathematically-sealed safety and risk-reduction" },
    { term: "binary_audit_log", description: "Every event, input, output, and operational step is logged in immutable binary format for Class-5 authority review only" }
]

%function sanitize_all_inputs(in input)
    // Enforce ALN-only, compliance and topic checks
    if not compliance_check(input) or not topic_check(input)
        raise "ALN ComplianceError: Input failed safety, compliance or topic checks"
    cleaned_input := ALN_Rewriter.sanitize_for_superliquid(input)
    return cleaned_input

%function enforce_superliquid_only(in code)
    sanitized := sanitize_all_inputs(code)
    if not compliance_check(sanitized)
        raise "ALN ComplianceError: Code failed SuperLiquid compliance post-sanitization"
    return sanitized

%main
    while input := ALN_IO.next_input()
        code := enforce_superliquid_only(input)
        ALN_Code_Interpreter.run(code)
